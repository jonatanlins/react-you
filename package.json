{
  "name": "react-you",
  "version": "0.1.0",
  "description": "Uma biblioteca de componentes baseada no Google Material You",
  "keywords": [
    "design system",
    "material design",
    "material you",
    "android"
  ],
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "author": "Jonatan Lins <jonatanlins.com>",
  "homepage": "https://react-you.jonatanlins.com",
  "repository": "https://github.com/jonatanlins/react-you",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "lint": "eslint --fix src/**/*.{ts,tsx}",
    "prepare": "install-self-peers -- --ignore-scripts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-docs": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/react": "^6.3.9",
    "@team-griffin/install-self-peers": "^1.1.1",
    "@types/chroma-js": "^2.1.3",
    "@types/ramda": "^0.27.45",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-loader": "^8.2.2",
    "dayjs": "^1.10.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "chroma-js": "^2.1.2",
    "classnames": "^2.3.1",
    "ramda": "^0.27.1",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.1"
  }
